#%Module1.0
#
proc ModulesHelp { } {
    global product version
    puts stderr "This module adds $product/$version to your environment."
}
#
# Define variables
#
set product legacysurvey
set version dr3-cori-scratch
conflict $product

#
# module-whatis
#
module-whatis "Sets up $product/$version in your environment."

set PRODUCT_DIR /global/cscratch1/sd/desiproc/dr3/

setenv LEGACY_SURVEY_DIR $PRODUCT_DIR
