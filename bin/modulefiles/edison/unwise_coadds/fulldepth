#%Module1.0
#
# $Id: 2.19.1 1998 2012-07-20 20:43:09Z weaver $
#
proc ModulesHelp { } {
    global product version
    puts stderr "This module adds $product/$version to your environment."
}
#
# Define variables
#
set product unwise_coadds
set version fulldepth
conflict $product
#
# module-whatis
#
module-whatis "Sets up $product/$version in your environment."
#

#
# Set environment
#

set PRODUCT_DIR /project/projectdirs/cosmo/data/unwise/neo1/unwise-coadds/fulldepth:/project/projectdirs/cosmo/data/unwise/unwise-coadds
setenv [string toupper $product]_DIR $PRODUCT_DIR
setenv unwise_coadds_VERSION $version

